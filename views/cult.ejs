      </form>
      <br><br>
    </div>
    <% if (name !=='' ) { %>
      <table style="margin-bottom: 20%;">
        <tr>
          <% for (let i=0; i < columns.length; i ++ ) { %>
            <th>
              <%= columns[i].name%>
            </th>
            <% } %>
        </tr>
        <% for (let i=0; i < tuples.length; i ++ ) { %>
          <tr>
            <%for (let attr in tuples[i] ) { %>
