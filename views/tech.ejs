  <section class="review" id="review">
    <!-- <img src="./sac images/sac about.jpg" justify-content="right" display="flex"> -->
    <div class="content" data-aos="fade-right">
      <h3>About Technical Committee</h3>
      <p>Technical Committee holds a strong belief of imparting knowledge to all, that can be used practically in real
        life. The committees main aim is to develop a technical aptitude amongst students of the college and develop
        good coding culture in the institute.
      <p>
    </div>

  </section>
  <h1 class="heading"> Everything about <span>Technical Committee</span> </h1>



  <div class="container">
    <div class="tables_list">

      <select class="dropdown" id="tables" name="name" form="tableForm">
        <option>Committee_Members</option>
        <option>events</option>
        <option>club</option>
        <option>equipments</option>
      </select>
      <form method="get" action="/know_more_tech" id="tableForm">
        <input type="submit" id="showBtn" class="showBtn">
      </form>
      <br><br>
    </div>
    <% if (name !=='' ) { %>
      <table style="margin-bottom: 20%;">
        <tr>
          <% for (let i=0; i < columns.length; i ++ ) { %>
            <th>
              <%= columns[i].name%>
            </th>
            <% } %>
        </tr>
        <% for (let i=0; i < tuples.length; i ++ ) { %>
          <tr>
            <%for (let attr in tuples[i] ) { %>
              <td>
                <%= tuples[i][attr] %>
              </td>
              <%} %>
          </tr>
          <% } %>

      </table>
      <%}%>
